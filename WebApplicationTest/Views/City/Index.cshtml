@using ConsoleAppTest.Data.Model

@model List<City>


<table id="table" class="table table-striped table-hover">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Code</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var city in Model)
        {
            <tr>
                <th scope="row">@city.Id</th>
                <td>@city.Name</td>
                <td>@city.Code</td>
            </tr>
        }
    </tbody>
</table>